{
  "compilerOptions":
    {
      "target": "ESNext",
      "module": "commonjs",
      "rootDir": "src",
      "outDir": "dist",
      "sourceMap": true,
      "resolveJsonModule": true,
      "lib": ["ESNext"],
      "esModuleInterop": true,
      "skipLibCheck": true
    },
  "include": [
    "src/**/*.ts"
  ],
  "exclude": ["node_modules"],
}